{
	"cSpell.language": "en-GB,pl",
	"latex-workshop.latex.tools": [
		{
			"name": "pdflatex_escape",
			"command": "pdflatex",
			"args": [
				"-shell-escape",
				"-synctex=1",
				"-interaction=nonstopmode",
				"-file-line-error",
				"%DOC%"
			],
			"env": {}
		},
		{
			"name": "bibtex",
			"command": "bibtex",
			"args": [
				"%DOCFILE%"
			],
			"env": {}
		}
	],
	"latex-workshop.latex.recipes": [
		{
			"name": "Build thesis",
			"tools": [
				"pdflatex_escape",
				"bibtex",
				"pdflatex_escape",
				"pdflatex_escape"
			]
		}
	],
	"cSpell.words": [
		"inkscapelatex"
	]
}
